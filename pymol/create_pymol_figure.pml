
bg white
set antialias, 2
set ray_trace_mode, 1
set ray_trace_color, black
set cartoon_side_chain_helper, on
set auto_zoom, off
set spec_reflect, 0
set ray_shadows,0

set sphere_quality, 2

set opaque_background, off
set ray_opaque_background, off

